- var TITLE = "TOP";
- var pageType = "top";
include ./include/config/_config
include ./include/compornent/_c-card

doctype html
html(lang="ja")
  include ./include/mixin/_head
    title
      | #{TITLE}
  body
    include ./include/layout/_header
    .top
      .top_area
        ul.top_list
          li.top_item
            +c-card({
              cardClass: "__kv",
              link: "/pages/kv-page-01.html",
              img: IMG_PATH + "image.jpg",
              alt: "kv画像",
              date: "2024.01.06",
              text: "loadingアニメーション",
            })
    include ./include/mixin/_script
